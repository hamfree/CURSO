<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="WS1">
      <implementation-class>ws.WS1</implementation-class>
    </service>
  </services>
  <clients>
    <client name="WS2">
      <wsdl-url>http://curso-pc:8080/Ejercicio_WS_WS2/WS2?wsdl</wsdl-url>
      <local-wsdl-file>curso-pc_8080/Ejercicio_WS_WS2/WS2.wsdl</local-wsdl-file>
      <package-name forceReplace="true">cws</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
